        <xs:sequence>
          <xs:element name="serviceProviderId" type="ServiceProviderId"/>
          <xs:element name="groupId" type="GroupId"/>
          <xs:element name="serviceUserId" type="UserId"/>
          <xs:element name="serviceInstanceProfile" type="ServiceInstanceAddProfile"/>
          <xs:element name="maximumBridgeParticipants" type="CollaborateBridgeMaximumParticipants"/>
          <xs:element name="networkClassOfService" type="NetworkClassOfServiceName" minOccurs="0"/>
          <xs:element name="maxCollaborateRoomParticipants" type="CollaborateRoomMaximumParticipants"/>
          <xs:element name="supportOutdial" type="xs:boolean"/>
          <xs:element name="collaborateOwnerUserId" type="UserId" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

